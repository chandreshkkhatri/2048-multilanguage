{
    "cli": {
        "version": ">= 3.10.2",
        "appVersionSource": "local"
    },
    "build": {
        "development": {
            "developmentClient": true,
            "distribution": "internal",
            "channel": "development",
            "env": {
                "APP_VARIANT": "development"
            }
        },
        "preview": {
            "distribution": "internal",
            "channel": "staging",
            "env": {
                "APP_VARIANT": "preview"
            }
        },
        "production": {
            "channel": "production"
        }
    },
    "submit": {
        "production": {}
    }
}
