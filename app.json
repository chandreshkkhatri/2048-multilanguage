{
    "expo": {
        "name": "2048-multilanguage",
        "slug": "2048-multilanguage",
        "version": "1.0.1",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "userInterfaceStyle": "light",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "assetBundlePatterns": [
            "**/*"
        ],
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "com.karmalok.multilingual2048"
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/adaptive-icon.png",
                "backgroundColor": "#E7B723"
            },
            "package": "com.karmalok.multilingual2048",
            "versionCode": 2,
            "targetSdkVersion": 35
        },
        "web": {
            "favicon": "./assets/favicon.png",
            "bundler": "metro",
            "output": "single",
            "name": "2048 Multilanguage",
            "backgroundColor": "#faf8ef",
            "themeColor": "#E9A067"
        },
        "updates": {
            "fallbackToCacheTimeout": 0,
            "url": "https://u.expo.dev/36e7f8cf-f2b8-47cc-9f2c-1e6b1160ff53"
        },
        "extra": {
            "eas": {
                "projectId": "36e7f8cf-f2b8-47cc-9f2c-1e6b1160ff53"
            }
        },
        "plugins": [
            ["expo-build-properties", {
                "android": {
                    "targetSdkVersion": 35
                }
            }]
        ],
        "newArchEnabled": false,
        "owner": "chandresh.code",
        "runtimeVersion": {
            "policy": "sdkVersion"
        }
    }
}